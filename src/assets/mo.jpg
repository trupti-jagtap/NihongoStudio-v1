    function gototop() {
        $('html, body').animate({scrollTop: 0}, 250);
    }

    function checkPosition() {
        for (let i = 0; i < elems.length; i++) {
            let positionFromTop = elems[i].getBoundingClientRect().bottom;
            if (positionFromTop - windowHeight <= 30) {
                // if we've already seen this element, ignore
                if (elems[i].getAttribute("shown")) continue;

                console.log("Element " + elems[i].id + " shown!");
                elems[i].setAttribute("shown", true);

                // now trigger animation
                switch (elems[i].id) {
                    case "anim_ss":
                        animateSsBadge();
                        break;
                    case "anim_jm":
                        mv0.draw(true);
                        break;
                    case "anim_map":
                        mm.animateOnReady();
                        break;
                    case "anim_wheel":
                        kw_0.highlightRing(2);
                        break;
                    case "anim_search":
                        $(document).ready(function () {
                            $('#image_search_overlay').css({
                                top: $('#anim_search').position().top,
                                width: $('#anim_search').width(),
                            });
                        });
                        // $('#image_search_overlay').css({
                        //     top: $('#image_search').position().top,
                        // });
                        $('#anim_search').animate({'opacity': 1}, 800);
                }
            }
        }
    }

    let elems;
    let windowHeight;

    $(document).ready(function () {
        $('#learn').click(function () {
            click('learn');
        });
        $('#start').click(function () {
            location.href = '/dashboard';
        });
        $('#dash').click(function () {
            location.href = '/dashboard';
