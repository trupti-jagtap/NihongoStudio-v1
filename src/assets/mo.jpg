<script>

    class MasteryView {

        // initialize with:
        // svg = id of svg container
        // data = array of values to display: [kanji, words, grammar, study points]
        //        - expressed as percentages from 0 to 100
        constructor(svg, data, mastery, showText) {
            this.debug = false;

            this.svg = svg;
            this.snap = Snap("#" + svg);

            this.radius = 100;
            this.strokeWidth = 2;
            this.offset = 3;

            // if we're displaying text, the center and viewbox are different
            this.showText = showText;
            if (showText) {
                this.centerX = 170;
                this.centerY = 140;
                this.snap.attr({viewBox: [0, 0, 350, 280].join(' ')});
            } else {
                // need room to expand
                this.centerX = 120;
                this.centerY = 120;
